{
  "build": {
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build",
    "devUrl": "http://localhost:5173",
    "frontendDist": "../dist"
  },
  "productName": "shadcn-admin-tauri",
  "version": "1.3.0",
  "identifier": "com.tauri.shadcn.admin",
  "bundle": {
    "active": true,
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "app": {
    "security": {
      "csp": null,
      "dangerousDisableAssetCspModification": true
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "Shadcn Admin",
        "width": 800,
        "height": 600
      }
    ]
  }
}