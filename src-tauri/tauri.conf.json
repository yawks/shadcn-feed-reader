{
  "build": {
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build",
    "devUrl": "http://localhost:5173",
    "frontendDist": "../dist"
  },
  "productName": "Shadcn Feed Reader",
  "version": "1.3.0",
  "identifier": "net.yawks.shadcn.feedreader",
  "bundle": {
    "active": true,
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "app": {
    "security": {
      "csp": null,
      "dangerousDisableAssetCspModification": true,
      "capabilities": [
        {
          "identifier": "main-capability",
          "description": "Capability for the main window",
          "windows": ["main"],
          "permissions": [
            "shell:allow-open"
          ]
        }
      ]
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "Shadcn Feed Reader",
        "width": 800,
        "height": 600,
        "label": "main"
      }
    ]
  },
  "plugins": {
    "shell": {
      "open": true
    }
  }
}